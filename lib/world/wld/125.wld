      <flags>0</flags>
      <sector>Field</sector>
      <special>NONE</special>
    </roombasic>
    <exit direction="north" keyword="" exitinfo="0" keynum="-1" toroom="12513">
      <description>A small farm can be seen by its lonesome self to the north.
</description>
    </exit>
    <exit direction="east" keyword="" exitinfo="0" keynum="-1" toroom="12635">
      <description>You see nothing special.
</description>
    </exit>
    <exit direction="south" keyword="" exitinfo="0" keynum="-1" toroom="12515">
      <description>The Caemlyn-Tar Valon road continues.
</description>
    </exit>
  </room>
  <room>
