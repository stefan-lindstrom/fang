    <exit direction="down" keyword="" exitinfo="0" keynum="0" toroom="24184">
      <description/>
    </exit>
  </room>
  <room>
    <roombasic title="The First Room of the Reserved Section" vnum="24186" zone="241">
      <roomdescription>Here, Ogier Teachers teach their students about the values of the 
history. Telling them that everyone should learn from our past mistake
to not do them again
</roomdescription>
      <flags>262274</flags>
      <sector>Inside</sector>
      <special>NONE</special>
    </roombasic>
