      <description/>
    </exit>
  </room>
  <room>
    <roombasic title="A Plain Room" vnum="14476" zone="144">
      <roomdescription>   The walls of this room are bare, save for a single beatifully
woven tapestry on the west wall.  Two plain benches are set near
the south end of the room, and there are doors to the north and
west.
</roomdescription>
      <flags>10</flags>
      <sector>Inside</sector>
      <special>NONE</special>
    </roombasic>
    <exit direction="north" keyword="door" exitinfo="15" keynum="14550" toroom="14471">
