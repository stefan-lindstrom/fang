    <exit direction="south" keyword="" exitinfo="0" keynum="0" toroom="14459">
      <description/>
    </exit>
  </room>
  <room>
    <roombasic title="The Stables" vnum="14452" zone="144">
      <roomdescription>   This is the west end of the stables, and to the south the last
division of the stalls can be seen.  A long open corridor stretches
eastward from here, towards the Morat quarters.
</roomdescription>
      <flags>2</flags>
      <sector>Inside</sector>
      <special>NONE</special>
    </roombasic>
    <exit direction="east" keyword="" exitinfo="0" keynum="0" toroom="14453">
      <description/>
    </exit>
