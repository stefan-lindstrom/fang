    <roombasic title="The Pikemen's Head Trainer's Tent" vnum="12033" zone="120">
      <roomdescription>When this position is filled, the trainer will give his own description.
</roomdescription>
      <flags>32776</flags>
      <sector>Inside</sector>
      <special>NONE</special>
