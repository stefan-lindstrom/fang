    <roombasic title="The Cavalry's Head Trainer's Tent" vnum="12039" zone="120">
      <roomdescription>When this position is filled, the trainer will make his own description.
</roomdescription>
      <flags>32776</flags>
      <sector>Inside</sector>
      <special>NONE</special>
    </roombasic>
    <exit direction="south" keyword="&lt;NONE&gt;" exitinfo="0" keynum="0" toroom="12038">
