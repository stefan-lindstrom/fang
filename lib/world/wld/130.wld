  <room>
    <roombasic title="The path up the mountain" vnum="13095" zone="130">
      <roomdescription>Fire didn't spare this path. Everything is burnt to a crisp.
</roomdescription>
      <flags>80</flags>
      <sector>Forest</sector>
      <special>NONE</special>
    </roombasic>
    <exit direction="down" keyword="&lt;NONE&gt;" exitinfo="0" keynum="0" toroom="13094">
