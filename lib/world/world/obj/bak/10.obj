  <object vnum="1033" name="book" special="NONE">
    <description>A book entitled &quot;Alcarin&apos;s Description of Combat&quot; is here.</description>
    <shortdesc>Alcarin&apos;s Description of Combat</shortdesc>
    <actiondesc/>
    <affectflag>0</affectflag>
    <objaffect location="NONE" modifier="0"/>
    <objaffect location="NONE" modifier="0"/>
    <objaffect location="NONE" modifier="0"/>
