  <object vnum="29803" name="monolith tall odd" special="NONE">
    <description>\c09A tall monolith is here, with a \c00\c03golden\c00\c09 plaque embedded in it.\c00</description>
    <shortdesc>a tall monolith</shortdesc>
    <actiondesc/>
    <affectflag>0</affectflag>
