  <room>
    <roombasic title="A Shop of Something" vnum="1367" zone="13">
      <roomdescription>Not quite sure yet, so don't hold yer breath ;p
</roomdescription>
      <flags>0</flags>
      <sector>City</sector>
      <special>NONE</special>
    </roombasic>
