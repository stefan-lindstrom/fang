  <room>
    <roombasic title="Entry hall to the Warder Barracks." vnum="3529" zone="35">
      <roomdescription>This hall is kept clean and neat, with no dust in evidence
anywhere.  The floors look freshly scrubbed.
Doors lead in all four directions here.
</roomdescription>
      <flags>32776</flags>
      <sector>Inside</sector>
      <special>NONE</special>
    </roombasic>
    <exit direction="north" keyword="&lt;NONE&gt;" exitinfo="1" keynum="0" toroom="3516">
