      <roomdescription>Vacant
</roomdescription>
      <flags>0</flags>
      <sector>Inside</sector>
      <special>NONE</special>
    </roombasic>
  </room>
  <room>
    <roombasic title="Path to the Meeting Hall" vnum="3587" zone="35">
      <roomdescription>This Room is not finished yet.
</roomdescription>
      <flags>32796</flags>
      <sector>Inside</sector>
      <special>NONE</special>
    </roombasic>
