    <mobprog main="OnSayTo" argtype="STRING" argument="yes, aes sedai">
      <sideterm number="Wearing" not="FALSE" argtype="STRING" argument="5010"/>
      <action number="SayTo" argtype="STRING" argument="very well, then."/>
      <action number="Say" argtype="STRING" argument="the first thing you must know is that an aes sedai&apos;s word is as law, to be heeded and respected."/>
      <action number="Pause" argtype="INT" argument="3"/>
      <action number="Say" argtype="STRING" argument="you must remember never to speak to an aes sedai unless she first speaks to you, or gives you permission to address her."/>
      <action number="Say" argtype="STRING" argument="while in a lesson with an accepted, she is as aes sedai until the lesson ends."/>
      <action number="Emote" argtype="STRING" argument="tamelle sedai taps her hip and smiles."/>
      <action number="Pause" argtype="INT" argument="2"/>
      <action number="Say" argtype="STRING" argument="if you understand all of this so far, please nod at me, child."/>
    </mobprog>
    <mobprog main="OnActionTo" argtype="STRING" argument="nod">
      <sideterm number="Wearing" not="FALSE" argtype="STRING" argument="5010"/>
      <action number="Action" argtype="STRING" argument="nod"/>
      <action number="SayTo" argtype="STRING" argument="good, we shall continue then."/>
      <action number="Pause" argtype="INT" argument="2"/>
      <action number="Say" argtype="STRING" argument="warders, trainees, students and accepted are all to be accorded respect only slightly less than that of an aes sedai."/>
      <action number="Say" argtype="STRING" argument="and lastly, a novice should learn to be meek, but at the same time be aware that few meek women are raised to aes sedai."/>
      <action number="Emote" argtype="STRING" argument="tamelle sedai slides her gaze over you, then winks."/>
      <action number="Pause" argtype="INT" argument="2"/>
      <action number="Say" argtype="STRING" argument="now, if you have any questions, take them up with another aes sedai, child.  i have precious little time for lackwits."/>
      <action number="Emote" argtype="STRING" argument="tamelle sedai waves a hand at you, turns and goes about her business."/>
